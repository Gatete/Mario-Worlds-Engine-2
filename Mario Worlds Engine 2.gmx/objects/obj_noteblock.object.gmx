          <argument>
            <kind>1</kind>
            <string>///Note block logic

//If this block has not been hit before.
if (ready == 0) {

    //If Mario does exist and is above this block.
    if (instance_exists(obj_mario))
    &amp;&amp; (obj_mario.y &lt; y-11)
    &amp;&amp; (obj_mario.state &lt; cs_state_jump) {
    
        //If Mario is overlapping the block.
        if (collision_rectangle(bbox_left,bbox_top-1,bbox_right,bbox_top-1,obj_mario,0,0)) {
        
            //Move down
            vspeed = 3;
            
            //Block hit
            ready = 1;
            
            //Hit from top
            top = true;
        }
    }
}

//Otherwise, if the block has been hit.
else if ((ready == 1) &amp;&amp; (top == true)) {

    //Prevent Mario from jumping if we didn't before.
    obj_mario.jumping = 2;
    
    //Move up
    vspeed -= 0.5;
    
    //Change the solid position
    mysolid.y = self.y;
}
</string>
