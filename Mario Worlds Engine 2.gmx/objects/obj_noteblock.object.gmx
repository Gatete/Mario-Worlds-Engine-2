            <kind>1</kind>
            <string>///Deals with block stuff

//If the block has been hit from the top.
if ((ready = 1) &amp;&amp; (top == true)) {

    //If the block is moving up and it's above the start position.
    if ((vspeed &lt; 0) &amp;&amp; (y &lt; ystart)) {
    
        //Create notes
        with (instance_create(bbox_left,bbox_top,obj_note)) motion_set(135,1);
        with (instance_create(bbox_right,bbox_top,obj_note)) motion_set(45,1);
            
        //Stop moving
        vspeed = 0;
        y = ystart;
        
        //Allow bump
        ready = 0;
        
        //Check if hit from top
        top = false;
        
        //If Mario is overlapping the block.
        if (collision_rectangle(bbox_left,bbox_top-5,bbox_right,bbox_top+4,obj_mario,0,0)) {
        
            //Play 'Spring' sound.
            audio_play_sound(snd_trampoline, 0, false);
                        
            //Set the vertical speed.
            obj_mario.vspeed = -4.125;
            
            //Let mario vary his jump.
            if (keyboard_check(vk_shift))
                obj_mario.jumping = 1;
            else
                obj_mario.jumping = 2;
        }
    }
}
</string>
