//Stop stream and timer
obj_levelmanager.alarm[1] = 1;

//Make Tides invisible
if (instance_exists(obj_tides)) {

    with (obj_tides) 
        visible = 0;
}

//Do not animate
image_speed = 0;
image_index = 0;

//Check if the background is drawn
ready = false;

