        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Item check

//If this is a leaf, replace with a real leaf
if (sprite_index == spr_leaf)
    instance_create(xstart,ystart,obj_leaf)

//Otherwise
else {

    //If the sprite is a beanstalk
    if (sprite_index == spr_beanstalk) {
    
        //Play 'Vine Sprout' sound.
        audio_play_sound(snd_vinesprout, 0, false);
        
        //Replace it with a real beanstalk.
        instance_create(xstart,ystart,obj_beanstalk);
    }
    
    //Otherwise, if the sprite is a yellow beanstalk
    else if (sprite_index == spr_beanstalk_alt) {
    
        //Play 'Vine Sprout' sound.
        audio_play_sound(snd_vinesprout, 0, false);
        
        //Replace it with a real yellow beanstalk.
        instance_create(xstart,ystart,obj_beanstalk_alt);        
    }
    else {

        //Play sprout sound
        audio_play_sound(snd_sprout, 0, false);
    
        //Exit this event.
        exit;
    }
}

//Destroy
instance_destroy()
</string>
