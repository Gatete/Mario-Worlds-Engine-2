            <string>///Finish sprouting

//Create a item out of the sprite.
switch (sprite_index) {

    //Mushroom
    case (spr_mushroom): {
    
        with (instance_create(x,y,obj_mushroom)) {
        
            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;
        }
    } break;
    
    //Fireflower
    case (spr_fireflower): {
    
        instance_create(x,y,obj_fireflower)
    } break;
            
    //1-UP
    case (spr_1up): {
    
        with (instance_create(x,y,obj_1up)) {
        
            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;
        }
    } break;
    
    //Poison
    case (spr_poison): {
    
        with (instance_create(x,y,obj_poison)) {
        
            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;
        }
    } break;          
    
    //Starman
    case (spr_starman): {
    
        with (instance_create(x,y,obj_starman)) {
        
            //If moving upwards
            if (other.vspeed &lt; 0) {
            
                if (!place_meeting(x,y,obj_swim))
                    vspeed = -5;
                else
                    vspeed = -1.25;
            }

            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;
        }
    } break;
}

//Destroy
instance_destroy();





</string>
