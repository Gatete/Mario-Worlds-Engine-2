        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Make Mario hold or interact with the item.

//If the item is not being held.
if (!held) { 

    //If Mario is above and moving down, use the stomp event.
    if (other.bbox_bottom &lt; bbox_top-vspeed+5) 
    &amp;&amp; (other.vspeed &gt; 0) {
    
        //Animate
        image_speed = 0.4;
        
        //Mario logic
        with (other) {
        
            //Stop horizontal speed.
            hspeed = 0;
            
            //Stop vertical speed.
            vspeed = 0;
            if (gravity &gt; 0)
                gravity = 0;
        }
        
        //Spring is ready
        ready = 1;
        s_ready = 1;
        
        //Mario logic (Part 2)
        with (other) event_user(15);
    }
    
    //Otherwise, if Mario can hold the item.
    else if (can_hold() &amp;&amp; (s_ready == 0) &amp;&amp; (!inwall) &amp;&amp; (other.bbox_bottom &gt; bbox_top)) {
        
        //Make Mario hold it.
        other.holding = 1;
        
        //Change the depth
        depth = -4;        
        
        //Get held
        held = 1;
                
        //Disable speed / gravity
        speed = 0;
        gravity = 0;
    }
}

//If the spring is ready.
if (ready) {

    //Set idle state
    obj_mario.state = cs_state_idle;    
    
    //Prevent Mario's gravity
    obj_mario.disablegrav = 1;

    //Prevent Mario from jumping.
    obj_mario.jumping = 2;
    
    //Stop sliding
    obj_mario.sliding = 0;
        
    //Set y position
    obj_mario.y = self.y+(image_index*2)-14;
}
</string>
          </argument>
        </arguments>
      </action>
