            <string>///Draw Mario

//If the animation has been set.
if (ready) {

    if (background_exists(back)) {
    
        //Disable alpha blending.
        draw_enable_alphablend(0);
        
        //Draw the screenshot
        draw_background(back,view_xview[0],view_yview[0]);
        
        //Enable alpha blending.
        draw_enable_alphablend(1);
    }
}

//If Mario is riding a yoshi
if (global.yoshi == 1) {

    //Set the palette
    pal_swap_set(spr_palette_yoshi, yoshicolour);
    
    //Draw Yoshi
    draw_sprite_ext(yoshisprite,0,round(x),round(y)+1,image_xscale,1,0,c_white,1);
    
    //Reset the shader
    shader_reset();
    
    //Draw Mario
    if (sequence == 3) {
    
        if (image_xscale == 1)
            draw_sprite_ext(sprite_index,-1,round(x)-4,round(y)-12,1,1,0,c_white,image_alpha);
        else if (image_xscale == -1)
            draw_sprite_ext(sprite_index,-1,round(x)+4,round(y)-12,1,1,0,c_white,image_alpha);
    }
    else {
    
        if (image_xscale == 1)
            draw_sprite_ext(sprite_index,-1,round(x)-4,round(y)-12,1,1,0,c_white,image_alpha);
        else
            draw_sprite_ext(sprite_index,-1,round(x)+4,round(y)-12,-1,1,0,c_white,image_alpha);
    }
}

else {

    if (sequence == 3)
        draw_sprite_ext(sprite_index,-1,round(x),round(y)+1,1,1,0,c_white,image_alpha);
    else
        draw_sprite_ext(sprite_index,-1,round(x),round(y)+1,image_xscale,1,0,c_white,image_alpha);
}
