            <string>///Mario got a powerup, change him.

//Sets and starts the animation sequences.
ready = 0;

//How many times does the animation loop?
loop = 0;

//Sets the animation to reproduce.
//0: Small to big
//1: Big to small
//2: Fire
//3: Poof
sequence = 0;

//Make Mario invisible
if (instance_exists(obj_mario)) {

    //Make Mario invisible.
    obj_mario.visible = false;
    
    //Snap to position.
    x = round(obj_mario.x);
    y = round(obj_mario.y);
    
    //Set the depth
    depth = obj_mario.depth;
    
    //Hereby Mario's facing direction.
    image_xscale = obj_mario.xscale;    
}

//Make Tides invisible
if (instance_exists(obj_tides)) {

    with (obj_tides) 
        visible = 0;
}

//Make Yoshi invisible
if (global.yoshi == 1) {

    yoshisprite = obj_yoshi.sprite_index;
    yoshicolour = obj_yoshi.colour;
    with (obj_yoshi)
        visible = 0;
}

//Start animations
alarm[0] = 1;
