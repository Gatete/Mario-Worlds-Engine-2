        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Get the adequate powerup.

//Check the sprite
switch (sprite_index) {

    //Mushroom
    case (spr_mushroom): {
        
        //If Mario is small.
        if (global.powerup == cs_small) {
        
            //Play 'Powerup' sound.
            audio_play_sound(snd_powerup, 0, false);
        
            //Turn Mario into 'Super' Mario.
            global.powerup = cs_big;
            
            //Perform animation sequence
            with (instance_create(0,0,obj_mario_transform))
                sequence = 0;
        }
        
        //Otherwise, get a reserve mushroom
        else {
        
            //Play 'Reserve' sound.
            audio_play_sound(snd_reserve, 0, false);
        
            //If NO item is in reserve.
            if (global.reserveitem = cs_small)
                global.reserveitem = cs_big;
        }        
    } break;
    
    //Fire Flower
    case (spr_fireflower): {
    
        //If Mario is big and there's NOT an item on reserve.
        if (global.powerup == cs_big) 
        &amp;&amp; (global.reserveitem == cs_small) {
        
            //If health mode is NOT enabled.
            if (global.healthmode == false)
                global.reserveitem = cs_big;
        }
        
        //Otherwise, if Mario does have a powerup.
        else if (global.powerup &gt; cs_big) {
        
            //Reserve previous powerup.
            global.reserveitem = global.powerup;
        }
        
        //If Mario is NOT Fire Mario.
        if (global.powerup != cs_fire) {
        
            //Play 'Powerup' sound.
            audio_play_sound(snd_powerup, 0, false);
        
            //Turn Mario into Fire Mario.
            global.powerup = cs_fire;
            
            //Perform animation sequence
            with (instance_create(0,0,obj_mario_transform))
                sequence = 2;
        }
        
        //Otherwise, if Mario is already Fire Mario
        else {
        
            //Play 'Reserve' sound
            audio_play_sound(snd_reserve, 0, false);
        }  
    } break;
    
    //Leaf
    case (spr_leaf): {
    
        //If Mario is big and there's NOT an item on reserve.
        if (global.powerup == cs_big) 
        &amp;&amp; (global.reserveitem == cs_small) {
        
            //If health mode is NOT enabled.
            if (global.healthmode == false)
                global.reserveitem = cs_big;
        }
        
        //Otherwise, if Mario does have a powerup.
        else if (global.powerup &gt; cs_big) {
        
            //Reserve previous powerup.
            global.reserveitem = global.powerup;
        }
        
        //If Mario is NOT Raccoon Mario.
        if (global.powerup != cs_leaf) {
        
            //Play 'Powerup' sound.
            audio_play_sound(snd_transform, 0, false);
        
            //Turn Mario into Raccoon Mario.
            global.powerup = cs_leaf;
            
            //Perform animation sequence
            with (instance_create(0,0,obj_mario_transform))
                sequence = 3;
        }
        
        //Otherwise, if Mario is already Fire Mario
        else {
        
            //Play 'Reserve' sound
            audio_play_sound(snd_reserve, 0, false);
        }
    } break;
}

//Destroy
instance_destroy();
</string>
