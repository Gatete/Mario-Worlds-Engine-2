        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Background control

//Title rotation
rotat += poop;
if (rota) {

    if (poop &gt; 0.46875) {
    
        poop = 0.46875;
        if (rotat &gt; 3)
            rota = 0;
    }
    else
        poop += 0.03125;
}
else {
    
    if (poop &lt; -0.46875) {
        poop = -0.46875;
        if (rotat &lt; -3)
            rota = 1;
    }
    else
        poop -= 0.03125;       
}

//Hue shift
hue += 0.15;
if (hue &gt; 255) {

    hue -= 255;
}

//Motion
background_hspeed[0] = -0.5;
background_vspeed[0] = -0.5;

//Colour
background_blend[0] = make_colour_hsv(hue,255,255);

//Motion
background_hspeed[1] = 0.5;
background_vspeed[1] = 0.5;

//Colour
background_blend[1] = merge_colour(c_white,make_colour_hsv(hue,255,255),0.5);
</string>
